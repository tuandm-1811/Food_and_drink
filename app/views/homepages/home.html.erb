<section class="block-preview">
  <div class="cover-banner" style="background-image: url(https://images3.alphacoders.com/922/922680.jpg)"></div>
  <div class="container">
    <div class="row">
      <div class="col-lg-8 col-md-6 col-sm-12">
        <div class="left-text-b">
          <h1 class="title"><%= t ".title"%></h1>
          <h6 class="exeption"><%= t ".exeption"%></h6>
          <p>Get our services from 24 hours.</p>
          <a class="bnr-btn btn-link" href="<%= products_path %>"><%= t ".gotomail"%></a>
        </div>
      </div>
    </div>
  </div>
</section>
