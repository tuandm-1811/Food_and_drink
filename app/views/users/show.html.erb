<section class="title-bar">
		<div class="container">
			<div class="row">
				<div class="col-md-6">
					<div class="left-title-text">
					<h3>My Account</h3>
					</div>
				</div>
				<div class="col-md-6">
					<div class="right-title-text">  
						<ul>
							<li class="breadcrumb-item">
								<%=link_to "Home", root_path %>
							</li>
							<li class="breadcrumb-item active" aria-current="page">My Account</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</section>
