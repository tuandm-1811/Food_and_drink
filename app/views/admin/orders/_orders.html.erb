<tr>
  <td><%= index +1 %></td>
  <td><%= order.name_product %></td>
  <td><%= order.user_id %></td>
  <td><%= order.cart_id %></td>
  <td><%= order.price %></td>
  <td><%= order.address_id %></td>
</tr>
<td>
  <%= link_to admin_order_path(product.order.id,product) do %>
    <i class='fas fa-edit' style='color: black'></i>
  <% end %>
  <%= link_to admin_order_path(product.order.id, product), remote: true, method: :delete, data: {confirm: "Bạn chắc chứ?"}, class:"delete-product" do %>
    <i class='far fa-trash-alt' style='color: black'></i>
  <% end %>
</td>